            if ($('#TimeFrom').val() != "") {
                let from = $('#TimeFrom').val();
                let to = $('#TimeTo').val();
                let miniutsFrom = from.split(':');
                let miniutsFromNumber = parseInt(miniutsFrom[0]) * 60 + parseInt(miniutsFrom[1]);
                let miniutsTo = to.split(':');
                let miniutsToNumber = parseInt(miniutsTo[0]) * 60 + parseInt(miniutsTo[1]);

                if (miniutsFromNumber > miniutsToNumber)
