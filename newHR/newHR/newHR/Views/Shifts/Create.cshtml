<script>
    var xhr = new XMLHttpRequest();
    console.log(xhr.readyState);
    xhr.onreadystatechange = function () {
        if (xhr.readyState == 4 && hr.status == 200) {
            updateUI(xhr.responseText);
        } else if (xhr.readyState == 4 && hr.status == 404) {
            alert("page not found")
        } else if (xhr.readyState == 4 && hr.status == 501) {
            alert("something went worng")
        }

    }
    document.getElementById('demo').onmouseover = function () {
        xhr.open("get", "index.html", true);
        xhr.send()
    }

    function updateUI() {
        document.getElementById("demo").textContent = text
    }

    $(function () {
        $("#FullCheck").change(function () {
            //   alert();
            if (this.checked) {
                $("#EmploymentPercent").removeAttr("disabled");
                $("#DailyHours").removeAttr("disabled");
                $("#WeekelyDays").removeAttr("disabled");
                $("#WeekelyHours").removeAttr("disabled");
                $("#MonthlyLeaveDays").removeAttr("disabled");
                $("#ShiftHoliday").removeAttr("disabled");

            }
            else {
                $("#EmploymentPercent").attr("disabled", "");
                $("#DailyHours").attr("disabled", "");
                $("#WeekelyDays").attr("disabled", "");
                $("#WeekelyHours").attr("disabled", "");
                $("#MonthlyLeaveDays").attr("disabled", "");
                $("#ShiftHoliday").attr("disabled", "");
            }
        });
    });

    $(function () {
        $("#WeekendCheck").change(function () {
            //   alert();
            if (this.checked) {
                $("#WeekendFromDay").removeAttr("disabled");
                $("#WeekendToDay").removeAttr("disabled");

            }
            else {
                $("#WeekendFromDay").attr("disabled", "");
                $("#WeekendToDay").attr("disabled", "");

            }
        });
    });

    $(function () {
        $("#StatusCheck").change(function () {
            //   alert();
            if (this.checked) {
                //$("#StartTime").removeAttr("disabled");
                //$("#EndTime").removeAttr("disabled");
                $("#LateArrive").removeAttr("disabled");
                $("#EarlyLeave").removeAttr("disabled");
                $("#EarlyArrive").removeAttr("disabled");
            } else {
                //$("#StartTime").attr("disabled", "");
                //$("#EndTime").attr("disabled", "");
                $("#LateArrive").attr("disabled", "");
                $("#EarlyLeave").attr("disabled", "");
