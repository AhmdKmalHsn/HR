@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model Model.Home

<div class="preview">
    @if (Model == null)
    {
        <img src="~/assets2/images/HR.jpg" style="width: 100%; height: 650px;" id="imgg">
    }
    else if (Model.Path != null)
    {
        <img src="~/assets2/images/@Model.Name" style="width: 100%; height: 650px;" id="imgg">
    }
    else
    {

    }
</div>
<script>
    var t = document.title;
    document.title = t + " - Home"
</script>

